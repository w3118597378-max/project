<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cl.dao.ForumTypeDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.cl.entity.ForumTypeEntity" id="forumTypeMap">
        <result property="typeName" column="type_name"/>
    </resultMap>

    <select id="selectListView"
		resultType="com.cl.entity.view.ForumTypeView" >

		SELECT  forum_type.* FROM forum_type  forum_type 	        
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectView"
		resultType="com.cl.entity.view.ForumTypeView" >
		SELECT * FROM forum_type  forum_type <where> 1=1 ${ew.sqlSegment}</where>
	</select>



</mapper>
